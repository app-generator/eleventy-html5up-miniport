---
layout : layouts/base.njk
---

		<!-- Home -->
			<article id="top" class="wrapper style1">
				<div class="container">
					<div class="row">
						<div class="col-4 col-5-large col-12-medium">
							<span class="image fit"><img src="/images/pic00.jpg" alt="" /></span>
						</div>
						<div class="col-8 col-7-large col-12-medium">
							<header>
								<h1>
                                <a href="{{ data.github }}">{{ data.app_title }}</a>
                                </h1>
							</header>
							<p>
                                Static Site prototyped on top of <a href="https://www.11ty.io/">Eleventy</a> SSG with Miniport Design
						        - 
						        <a href="{{ data.github }}"> source code </a>                                
                            </p>
							<a href="#work" class="button large scrolly">Learn more</a>
						</div>
					</div>
				</div>
			</article>

		<!-- Work -->
			<article id="work" class="wrapper style2">
				<div class="container">
					<header>
						<h2>
                        Boilerplate Features
                        </h2>
						<p>Odio turpis amet sed consequat eget posuere consequat.</p>
					</header>
					<div class="row aln-center">
						<div class="col-4 col-6-medium col-12-small">
							<section class="box style1">
								<span class="icon featured fa-comments"></span>
								<h3>
                                    Built with Eleventy
                                </h3>
								<p>
                                    An elegant JavaScript alternative to Jekyll. 
                                </p>
							</section>
						</div>
						<div class="col-4 col-6-medium col-12-small">
							<section class="box style1">
								<span class="icon solid featured fa-camera-retro"></span>
								<h3>
                                    Open-Source
                                </h3>
								<p>
                                    Just grab this boilerplate and build something great.
                                </p>
							</section>
						</div>
						<div class="col-4 col-6-medium col-12-small">
							<section class="box style1">
								<span class="icon featured fa-thumbs-up"></span>
								<h3>
                                    Modern Tooling
                                </h3>
								<p>
                                    Webpack, Babel, Sass/SCSS, PostCSS, Light-server
                                </p>
							</section>
						</div>
					</div>
					<footer>
						<p>
                            Generated with automation tools by AppSeed 
                        </p>
						<a href="#portfolio" class="button large scrolly">My recent work</a>
					</footer>
				</div>
			</article>

		<!-- Portfolio -->
			<article id="portfolio" class="wrapper style3">
				<div class="container">
					<header>
						<h2>Stuff I made recently.</h2>
						<p>Proin odio consequat  sapien vestibulum consequat lorem dolore feugiat.</p>
					</header>
					<div class="row">
						<div class="col-4 col-6-medium col-12-small">
							<article class="box style2">
								<a href="{{ data.github }}" class="image featured"><img src="/images/pic01.jpg" alt="" /></a>
								<h3><a href="{{ data.github }}">Source code</a></h3>
								<p>the source code for this boilerplate</p>
							</article>
						</div>
						<div class="col-4 col-6-medium col-12-small">
							<article class="box style2">
								<a href="{{ data.github_group }}" class="image featured"><img src="/images/pic02.jpg" alt="" /></a>
								<h3><a href="{{ data.github_group }}">More Static Sites</a></h3>
								<p>more generated boilerplates</p>
							</article>
						</div>
						<div class="col-4 col-6-medium col-12-small">
							<article class="box style2">
								<a href="https://www.11ty.io/" class="image featured"><img src="/images/pic03.jpg" alt="" /></a>
								<h3><a href="https://www.11ty.io/">Eleventy</a></h3>
								<p>read more about this amazing SSG</p>
							</article>
						</div>
					</div>
					<footer>
						<p>Lorem ipsum dolor sit sapien vestibulum ipsum primis?</p>
						<a href="#contact" class="button large scrolly">Get in touch with me</a>
					</footer>
				</div>
			</article>

		<!-- Contact -->
			<article id="contact" class="wrapper style4">
				<div class="container medium">
					<header>
						<h2>Have me make stuff for you.</h2>
						<p>Ornare nulla proin odio consequat sapien vestibulum ipsum.</p>
					</header>
					<div class="row">
						<div class="col-12">
							<form method="post" action="#">
								<div class="row">
									<div class="col-6 col-12-small">
										<input type="text" name="name" id="name" placeholder="Name" />
									</div>
									<div class="col-6 col-12-small">
										<input type="text" name="email" id="email" placeholder="Email" />
									</div>
									<div class="col-12">
										<input type="text" name="subject" id="subject" placeholder="Subject" />
									</div>
									<div class="col-12">
										<textarea name="message" id="message" placeholder="Message"></textarea>
									</div>
									<div class="col-12">
										<ul class="actions">
											<li><input type="submit" value="Send Message" /></li>
											<li><input type="reset" value="Clear Form" class="alt" /></li>
										</ul>
									</div>
								</div>
							</form>
						</div>
						<div class="col-12">
							<hr />
							<h3>Links</h3>
							<ul class="social">
								<li><a href="{{ data.github   }}" class="icon brands fa-github"><span class="label">Github</span></a></li>
								<li><a href="{{ data.twitter  }}" class="icon brands fa-twitter"><span class="label">Twitter</span></a></li>
								<li><a href="{{ data.facebook }}" class="icon brands fa-facebook-f"><span class="label">Facebook</span></a></li>
							</ul>
							<hr />
						</div>
					</div>
                    
                    {% include 'footer.njk' %}
                    
				</div>

			</article>
